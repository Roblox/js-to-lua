{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "convert-js-to-lua",
  "projects": {
    "playground": {
      "root": "apps/playground",
      "sourceRoot": "apps/playground/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/playground",
            "main": "apps/playground/src/main.ts",
            "tsConfig": "apps/playground/tsconfig.app.json",
            "assets": ["apps/playground/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/playground/src/environments/environment.ts",
                  "with": "apps/playground/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "playground:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/playground/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/playground"],
          "options": {
            "jestConfig": "apps/playground/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "js-to-lua": {
      "root": "libs/js-to-lua",
      "sourceRoot": "libs/js-to-lua/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/js-to-lua/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/js-to-lua"],
          "options": {
            "jestConfig": "libs/js-to-lua/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "convert-js-to-lua": {
      "root": "apps/convert-js-to-lua",
      "sourceRoot": "apps/convert-js-to-lua/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/convert-js-to-lua",
            "main": "apps/convert-js-to-lua/src/main.ts",
            "tsConfig": "apps/convert-js-to-lua/tsconfig.app.json",
            "assets": [
              "apps/convert-js-to-lua/src/assets",
              "apps/convert-js-to-lua/src/fixtures"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/convert-js-to-lua/src/environments/environment.ts",
                  "with": "apps/convert-js-to-lua/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "convert-js-to-lua:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/convert-js-to-lua/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/convert-js-to-lua"],
          "options": {
            "jestConfig": "apps/convert-js-to-lua/jest.config.js",
            "passWithNoTests": true
          },
          "configurations": {
            "conformance": {
              "jestConfig": "apps/convert-js-to-lua/jest-conformance.config.js"
            }
          }
        },
        "e2e": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/convert-js-to-lua"],
          "options": {
            "jestConfig": "apps/convert-js-to-lua/jest-conformance.config.js",
            "passWithNoTests": true
          }
        },
        "verify": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node tools/scripts/verify-conformance-tests.js"
          }
        }
      }
    },
    "lua-types": {
      "root": "libs/lua-types",
      "sourceRoot": "libs/lua-types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/lua-types/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/lua-types"],
          "options": {
            "jestConfig": "libs/lua-types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "stylua-wasm": {
      "root": "libs/stylua-wasm",
      "sourceRoot": "libs/stylua-wasm/src",
      "projectType": "library",
      "targets": {}
    },
    "shared-utils": {
      "root": "libs/shared-utils",
      "sourceRoot": "libs/shared-utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared-utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared-utils"],
          "options": {
            "jestConfig": "libs/shared-utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
