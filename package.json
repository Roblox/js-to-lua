    "@babel/preset-typescript": "^7.15.0",
    "@babel/traverse": "^7.18.2",
    "@babel/types": "7.15.0",
    "@types/ramda": "^0.27.40",
    "glob": "^7.1.6",
    "ramda": "^0.27.1",
    "stylua-wasm": "file:libs/stylua-wasm/pkg",
    "tslib": "^2.0.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@nrwl/cli": "13.2.1",
