    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "jest": "27.2.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.4.1",
    "ts-jest": "27.0.5",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
