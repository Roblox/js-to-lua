{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/upstream-utils/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/upstream-utils",
        "main": "libs/upstream-utils/src/index.ts",
        "tsConfig": "libs/upstream-utils/tsconfig.lib.json",
        "assets": ["libs/upstream-utils/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/upstream-utils/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/upstream-utils"],
      "options": {
        "jestConfig": "libs/upstream-utils/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:entry", "feature:upstream"]
}
